# Gmail search query
gmail_query=newer_than:365d

# Google OAuth client secret JSON path
google_client_secret_path=client_secret.json

# OAuth token path
google_token_path=token.json

# Google Calendar ID for YogiSync
yogisync_calendar_id=YOUR_CALENDAR_ID

# Optional: override timezone
timezone=Asia/Tokyo

# Default duration for timed events (minutes)
default_event_duration_minutes=60

# SQLite DB path
sqlite_path=data/yogisync.db
